// SECTION :: START

section#real.real
  .c-grid
    .real__wrapper
      .real__wrapper-left.wow.animate__animated.animate__fadeInUp
        img(src="img/img-real-1.jpg" srcset="img/img-real-1@2x.jpg 2x" alt="" parallax-js data-stellar-ratio="1.025")

      .real__wrapper-middle
        div.wow.animate__animated.animate__fadeInUp

          .real__content.wow.animate__animated.animate__fadeInUp

            h2.real__title.c-title.c-title--small REAL PATIENTS
            p.real__label.c-label BEFORE AND AFTER PHOTOS
            p.real__desc.c-desc Dr. Hubert has extensive expertise, years of experience, and particular interest in breast surgery. From breast augmentation with implants, to breast lifts, breast reduction, and breast reconstruction for women with cancer, Dr. Hubert has the ability to provide you with the individualized approach which is right for you.

            .real__btn.c-btn__wrapper.c-btn__wrapper--center
              a.c-btn(href="#")
                span VIEW GALLERY

      .real__wrapper-right.wow.animate__animated.animate__fadeInUp
        img(src="img/img-real-2.jpg" srcset="img/img-real-2@2x.jpg 2x" alt="" parallax-js data-stellar-ratio="1.025")

// SECTION :: END
